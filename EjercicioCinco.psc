Proceso EjercicioCinco
	Definir kilometros, litros, consumo Como Real
	Escribir "Ingrese la cantidad de litros que ha cargado"
	Leer litros
	Escribir "Ingrese la cantidad de kilometros que ha recorrido"
	Leer kilometros
	consumo <- kilometros/litros
	Escribir "Por cada kilometro se consumen ",consumo," litros"
FinProceso
