Proceso EjercicioSeis
	Definir numUno, numDos, numAuxUno, numAuxDos Como Real
	Escribir "Ingrese el valor uno"
	Leer numUno
	Escribir "Ingrese el valor dos"
	Leer numDos
	numAuxUno <- numDos
	numAuxDos <- numUno
	Escribir "Procedo a invertir los valores"
	Escribir "Nuevo valor uno : ", numAuxUno
	Escribir "Nuevo valor dos : ", numAuxDos
	
FinProceso